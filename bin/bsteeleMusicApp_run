#!/bin/tcsh
#set echo; set verbose

set app=bsteeleMusicApp
set git=~/GitHub/bsteeleMusicFlutter


cd $git
set runner=$home/GitHub/bsteeleMusicFlutter/build/linux/debug/bundle/bsteeleMusicFlutter
if ( -e $runner ) then
	echo running old version:
	ll $runner
	$runner
else
	flutter -d linux run
endif
